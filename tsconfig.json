{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"baseUrl": "src",
		"paths": {
			"@lib/*": ["lib/*"],
			"@utils/*": ["utils/*"],
			"@components/*": ["components/*"],
			"@layouts/*": ["layouts/*"],
			"@assets/*": ["assets/*"],
			"@pages/*": ["pages/*"],
			"@assets": ["assets/"]
		}
	}
}
